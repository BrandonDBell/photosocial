<br />
<br />
<div class="container result">
  <div class="two-thirds column">
      <h1><%= @title %>:</h1>
      <% if @users.any? %>
        <ul>
          <%= render @users %>
        </ul>  
        <%= will_paginate @users %>
      <% end %>
  </div>
  <div class="one-third column">
      <strong>Name</strong> <%= @user.username %><br />
      <strong>URL</strong> <%= link_to user_path(@user), @user %><br />
      <%= render 'shared/stats' %>
      <% if @users.any? %>
        <% @users.each do |user| %>
          <%= link_to image_tag(user.avatar.url(:profileimg)), user %>
        <% end %>
      <% end %>
  </div>
</div>